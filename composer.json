{
    "name": "dalyio/laravel-gitpack",
    "description": "A package for Laravel to perform basic git commands on locally integrated development packages",
    "homepage": "https://github.com/dalyio/laravel-gitpack",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Daly",
            "email": "scdaly24@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Dalyio\\Gitpack\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dalyio\\Gitpack\\Providers\\GitpackServiceProvider"
            ]
        },
        "branch-alias": {
            "main": "0.0.2",
            "master": "0.0.2"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
